{
    "__comment__": "Experiment",
    "project_root": "",
    "data_root": "",
    "wav_dir": "",
    "list_dir": "",
    "wav_list": "",
    "results_save_dir": "",
    "results_save_filename": "",
    "optim_patience": 5,
    "plot_results": false,  
    "step_size": [
        0.05,
        0.1,
        0.15,
        0.2,
        0.25,
        0.3,
        0.35,
        0.4,
        0.45,
        0.5
    ],
    "__comment__": "Log",
    "verbose": false,
    "log_dir": "",
    "log": {
        "version": 1,
        "disable_existing_loggers": false,
        "formatters": {
            "custom": {
                "format": "%(asctime)s - [%(name)s - %(funcName)-8.8s:%(lineno)-4d] - [%(levelname)-5.5s] - %(message)s",
                "datefmt": "%Y/%m/%d %H:%M"
            }
        },
        "handlers": {
            "console": {
                "class": "logging.StreamHandler",
                "level": "DEBUG",
                "formatter": "custom",
                "stream": "ext://sys.stdout"
            },
            "file": {
                "class": "logging.handlers.RotatingFileHandler",
                "level": "DEBUG",
                "formatter": "custom",
                "filename": "",
                "maxBytes": 10485760,
                "backupCount": 20,
                "encoding": "utf8"
            }
        },
        "loggers": {
            "main": {
                "level": "DEBUG",
                "handlers": [
                    "console",
                    "file"
                ],
                "propagate": false
            }
        },
        "root": {
            "level": "DEBUG",
            "handlers": []
        }
    }
}